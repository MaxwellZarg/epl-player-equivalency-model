.env
config/secrets.yaml

# Jupyter Notebook checkpoints
.ipynb_checkpoints/

# IDE files
.vscode/settings.json
.idea/

# OS files
.DS_Store
Thumbs.db

# Large files
*.zip
*.tar.gz
data/*.zip
