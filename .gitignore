cat >> .gitignore << 'EOF'

# Data files 
data/raw/
data/processed/
*.csv
*.xlsx
*.json
*.pkl
*.joblib

# Model files
models/trained/
models/*.pkl
models/*.joblib

# Results and outputs
results/temp/
*.log

